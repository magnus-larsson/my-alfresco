<documentList>
  <!-- <![CDATA[ @overridden projects/slingshot/config/alfresco/site-webscripts/org/alfresco/components/document-details/document-actions.get.config.xml 
    ]]> -->
  <actionSets>
    <actionSet id="empty"></actionSet>

    <actionSet id="document">
      <action type="action-link" id="onActionDownload" href="{downloadUrl}" label="actions.document.download-reading-copy" />
      <!-- <action type="simple-link" id="onActionView" href="{viewUrl}" label="actions.document.view" /> -->
      <action type="action-label" id="action-label" permission="edit" label="actions.document.label.edit" />
      <action type="simple-link" id="onActionDetails" permission="edit" href="{editMetadataUrl}" label="actions.document.edit-metadata" />
      <!-- <action type="action-link" id="onActionCheckInNewVersion" permission="edit" label="actions.document.upload-new-version" /> -->
      <!-- <action type="simple-link" id="onActionInlineEdit" permission="edit,inline-edit" href="{inlineEditUrl}" label="actions.document.inline-edit" 
        /> -->
      <action type="action-link" id="onActionEditOnline" permission="edit,online-edit" label="actions.document.edit-online" />
      <action type="action-link" id="onActionEditOffline" permission="edit,~googledocs-edit" label="actions.document.edit-offline" />
      <!-- <action type="action-link" id="onActionCheckoutToGoogleDocs" permission="edit,googledocs-edit" label="actions.document.checkout-google" /> <action 
        type="action-link" id="onActionSimpleApprove" permission="simple-approve" label="actions.document.simple-approve" /> <action type="action-link" id="onActionSimpleReject" 
        permission="simple-reject" label="actions.document.simple-reject" /> -->


      <action type="action-label" id="action-label" permission="admin,~document-publisher,unpublished,~older-version-published" label="actions.document.label.publish" />
      <action type="action-label" id="action-label" permission="document-publisher,unpublished,~older-version-published" label="actions.document.label.publish" />
      <action type="action-label" id="action-label" permission="admin,~document-publisher,published,~older-version-published" label="actions.document.label.publish" />
      <action type="action-label" id="action-label" permission="document-publisher,published,~older-version-published" label="actions.document.label.publish" />
      <action type="action-label" id="action-label" permission="document-publisher,older-version-published" label="actions.document.label.publish" />

      <action type="action-link" id="onActionPublishToStorage" permission="admin,~document-publisher,unpublished" label="actions.document.publish-to-storage" />
      <action type="action-link" id="onActionPublishToStorage" permission="document-publisher,unpublished" label="actions.document.publish-to-storage" />
      <action type="action-link" id="onActionUnpublishFromStorage" permission="admin,~document-publisher,published" label="actions.document.unpublish-from-storage" />
      <action type="action-link" id="onActionUnpublishFromStorage" permission="document-publisher,published" label="actions.document.unpublish-from-storage" />
      <action type="action-link" id="onActionUnpublishFromStorage" permission="document-publisher,older-version-published" label="actions.document.unpublish-old-from-storage" />



      <action type="action-label" id="action-label" label="actions.document.label.copy" />
      <action type="action-link" id="onActionCopyTo" label="actions.document.copy-to" />
      <action type="action-link" id="onActionMoveTo" permission="delete" label="actions.document.move-to" />
      <action type="action-link" id="onActionDelete" permission="delete" label="actions.document.delete" />

      <!-- <action type="action-link" id="onActionAssignWorkflow" label="actions.document.assign-workflow" /> -->
      <action type="action-link" id="onActionManagePermissions" permission="permissions" label="actions.document.manage-permissions" />
      <!-- <action type="action-link" id="onActionManageAspects" permission="edit" label="actions.document.manage-aspects" /> <action type="action-link" 
        id="onActionChangeType" permission="edit" label="actions.document.change-type" /> -->
    </actionSet>

    <actionSet id="locked">
      <action type="simple-link" id="onActionDownload" href="{downloadUrl}" label="actions.document.download.reading-copy" />
      <action type="simple-link" id="onActionViewWorkingCopy" permission="view-working-copy" href="{workingCopyUrl}" label="actions.document.view-working-copy" />

      <action type="action-label" id="action-label" permission="edit" label="actions.document.label.edit" />
      <action type="action-link" id="onActionCancelEditing" permission="cancel-checkout" label="actions.document.cancel-editing" />
      <!-- <action type="simple-link" id="onActionView" href="{viewUrl}" label="actions.document.view" /> -->


      <action type="action-label" id="action-label" label="actions.document.label.origin" />
      <action type="simple-link" id="onActionViewOriginal" permission="view-original" href="{originalUrl}" label="actions.document.view-original" />

    </actionSet>

    <actionSet id="lockOwner">
      <action type="simple-link" id="onActionDownload" href="{downloadUrl}" label="actions.document.download.reading-copy" />
      <!-- <action type="simple-link" id="onActionView" href="{viewUrl}" label="actions.document.view" /> -->
      <action type="simple-link" id="onActionViewWorkingCopy" permission="view-working-copy" href="{workingCopyUrl}" label="actions.document.view-working-copy" />
    </actionSet>

    <actionSet id="workingCopyOwner">
      <action type="simple-link" id="onActionDownload" href="{downloadUrl}" label="actions.document.download.working-copy" />

      <action type="action-label" id="action-label" permission="edit" label="actions.document.label.edit" />
      <action type="simple-link" id="onActionDetails" permission="edit" href="{editMetadataUrl}" label="actions.document.edit-metadata" />
      <!-- <action type="simple-link" id="onActionInlineEdit" permission="edit,inline-edit" href="{inlineEditUrl}" label="actions.document.inline-edit" 
        /> -->
      <action type="action-link" id="onActionCheckInNewVersion" label="actions.document.upload-new-version" />
      <!-- <action type="simple-link" id="onActionView" href="{viewUrl}" label="actions.document.view" /> -->
      <action type="action-link" id="onActionCancelEditing" permission="edit" label="actions.document.cancel-editing" />

      <action type="action-label" id="action-label" label="actions.document.label.origin" />
      <action type="simple-link" id="onActionViewOriginal" permission="view-original" href="{originalUrl}" label="actions.document.view-original" />
    </actionSet>

    <actionSet id="link">
      <action type="action-link" id="onActionDelete" permission="delete" label="actions.link.delete" />
    </actionSet>

    <actionSet id="googleDocLocked">
      <action type="simple-link" id="onActionView" href="{viewGoogleDocUrl}" label="actions.document.view-google-doc" />
      <action type="simple-link" id="onActionViewWorkingCopy" permission="view-working-copy" href="{workingCopyUrl}" label="actions.document.view-working-copy" />
      <action type="simple-link" id="onActionViewOriginal" permission="view-original" href="{originalUrl}" label="actions.document.view-original" />
      <action type="action-link" id="onActionCancelEditing" permission="cancel-checkout" label="actions.document.cancel-editing" />
    </actionSet>

    <actionSet id="googleDocOwner">
      <action type="simple-link" id="onActionView" href="{viewGoogleDocUrl}" label="actions.document.view-google-doc" />
      <action type="action-link" id="onActionCheckinFromGoogleDocs" permission="checkin-from-google" label="actions.document.checkin-google" />
      <action type="simple-link" id="onActionViewWorkingCopy" permission="view-working-copy" href="{workingCopyUrl}" label="actions.document.view-working-copy" />
      <action type="simple-link" id="onActionViewOriginal" permission="view-original" href="{originalUrl}" label="actions.document.view-original" />
      <action type="action-link" id="onActionCancelEditing" permission="edit" label="actions.document.cancel-editing" />
    </actionSet>

    <actionSet id="transferredDocument">
      <action type="simple-link" id="onActionDownload" href="{downloadUrl}" label="actions.document.download" />
      <action type="simple-link" id="onActionView" href="{viewUrl}" label="actions.document.view" />
      <action type="simple-link" id="onActionViewSourceRepository" permission="view-source-repository,~portlet" href="{sourceRepositoryUrl}"
        label="actions.document.view-source-repository" />
    </actionSet>
  </actionSets>
</documentList>
