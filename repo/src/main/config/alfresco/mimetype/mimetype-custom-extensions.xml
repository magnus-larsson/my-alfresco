<alfresco-config area="mimetype-map">

  <config evaluator="string-compare" condition="Mimetype Map">
    <mimetypes>
      <mimetype mimetype="application/vnd.ms-excel.sheet.macroenabled.12" display="Microsoft Excel 2007 Macro Enabled">
        <extension>xlsm</extension>
      </mimetype>
      <mimetype mimetype="video/mpeg" display="HDD Camcorder Video Data">
        <extension>mod</extension>
      </mimetype>
    </mimetypes>
  </config>

</alfresco-config>
