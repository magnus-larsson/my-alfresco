<?xml version='1.0' encoding='UTF-8'?>

<ehcache>

    <!-- defaults -->
    <diskStore 
        path="java.io.tmpdir"
    />

    <defaultCache 
        maxElementsInMemory="5000" 
        eternal="false" 
        timeToIdleSeconds="86400" 
        timeToLiveSeconds="86400" 
        overflowToDisk="true" 
        >
    </defaultCache>

    <cache name="se.vgregion.alfresco.repo.cache.APELON_CACHE"
        maxElementsInMemory="5000"
        eternal="false"
        timeToIdleSeconds="86400"
        timeToLiveSeconds="86400"
        overflowToDisk="true"
        />
        
</ehcache>